@model IEnumerable<fqtd.Areas.Admin.Models.SP_SearchByCategory_Result>

@{
    ViewBag.Title = "SearchByCategory";
}
<div class="caption">
    
    @Html.ActionLink("   List ALL   ", "Index")|
    @Html.ActionLink("   SearchByMode   ", "SearchByMode")|
        @Html.ActionLink("SearchByKeyword", "SearchByKeyword")|
        @Html.ActionLink("SearchByBrand", "SearchByBrand")

</div>
<h2>SearchByCategory</h2>


<table class="table_list">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SearchCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResultCount)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SearchCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResultCount)
        </td>
    </tr>
}

</table>
